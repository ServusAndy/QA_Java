package com.telran.data;

public class Person {
		private String name;
		private int age;
		private String id;
		
		public Person(String name , int age, String id) {
			this.name = name;
			this.age = age;
			this.id = id;
			
		}
		
		void setName(String name) {
			this.name = name;
		}
		
		protected String getName() {
			return name;
		}
		
		
		
		public int getAge() {
			return age;
		}

		public void setAge(int age) {
			this.age = age;
		}

		public String getId() {
			return id;
		}

		public void setId(String id) {
			this.id = id;
		}

		public String toString() {
			return name + " " + age + " " + id;
		}
}
