import java.util.ArrayList;

public class ArrayListTest {

	public static void main(String[] args) {
		ArrayList<String> cats = new ArrayList<>();
		System.out.println(cats.isEmpty());
		cats.add("Murzik");
		System.out.println(cats.size());
		cats.add("Kisya");
		cats.add("Pussy");
		System.out.println(cats.size());
	}

}
