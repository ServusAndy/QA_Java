		Book b4 = new Book("K.Smith","Klerks",211,180.,false);
		ArrayList<Book> books = new ArrayList<>();
		books.add(b);
		books.add(b1);
		books.add(b2);
		books.add(b3);
		books.add(b4);
		
		Arrays.sort(books,(Book bk,Book bk1)->{
			int res = bk.
		});
	
		
		
		
	}

}
