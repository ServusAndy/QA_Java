import java.util.ArrayList;

public class Main {

	public static void main(String[] args) {
		
		ArrayList<String> myList = new ArrayList<>();
		boolean res = myList.add("Vasya");
		myList.add(0, "Petay");
		String str = myList.get(1);
		System.out.println(str);
		
		str = myList.set(0, "Sofia");
		System.out.println(str);
		
		
	}

}
